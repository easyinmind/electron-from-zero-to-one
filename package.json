{
  "name": "share",
  "version": "0.1.0",
  "private": true,
  "main": "main.js",
  "description": "A Demo",
  "author": {
    "name": "easyinmind",
    "email": "easyinmind@123.com"
  },
  "homepage": "./",
  "dependencies": {
    "electron-is-dev": "^1.2.0",
    "electron-store": "^5.1.1"
  },
  "scripts": {
    "start-render": "react-app-rewired start",
    "start-main": "nodemon --watch main.js  --exec electron . ",
    "start": "concurrently 'wait-on http://localhost:3000 && npm run start-main' 'cross-env BROWSER=none npm run start-render'",
    "build-render": "react-app-rewired build",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "prepack": "npm run build-render",
    "predist": "npm run build-render",
    "test": "react-app-rewired test",
    "eject": "react-app-rewired eject"
  },
  "build": {
    "appId": "suji",
    "productName": "速记",
    "copyright": "Copyright © 2020 ${author}",
    "extends": null,
    "publish": ["github"],
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "main.js",
      "package.json"
    ],
    "mac": {
      "category": "public.app-category.productivity",
      "artifactName": "${productName}-${version}-${arch}.${ext}"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "antd": "^4.2.5",
    "babel-plugin-import": "^1.13.0",
    "concurrently": "^5.2.0",
    "cross-env": "^7.0.2",
    "customize-cra": "^1.0.0",
    "electron": "^13.6.6",
    "electron-builder": "^22.7.0",
    "less": "^3.11.1",
    "less-loader": "^6.1.0",
    "react-app-rewired": "^2.1.6",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1",
    "wait-on": "^5.0.0"
  }
}
